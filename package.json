{
  "name": "uniforms-antd5",
  "version": "4.0.0-alpha.5",
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": [
    "cjs/wrapField.js",
    "esm/wrapField.js",
    "src/wrapField.tsx"
  ],
  "description": "Ant Design v.5 UI components for uniforms.",
  "repository": "https://github.com/pivotdude/uniforms-antd5/tree/master/packages/uniforms-antd",
  "bugs": "https://github.com/pivotdude/uniforms-antd5//issues",
  "keywords": [
    "antd",
    "form",
    "forms",
    "meteor",
    "react",
    "react-component",
    "schema",
    "validation"
  ],
  "files": [
