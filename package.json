  "peerDependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.12.1",
    "react": "^18.2.0"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "uniforms": "^4.0.0-alpha.5",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.37",
    "@types/lodash": "^4.14.202",
